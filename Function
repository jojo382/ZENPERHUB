    while _G.KillCombat do wait()
        changestate()
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - game.Players[Select].Character.HumanoidRootPart.CFrame * CFrame.new(0,-10,0).Position).magnitude/300, Enum.EasingStyle.Linear)-- 10 คือ ระยะเวลาที่จะไปถึงจุด ...
    tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = game.Players[Select].Character.HumanoidRootPart.CFrame * CFrame.new(0,-10,0)}) -- CFrame เรา ไป CFrame.new
    tween:Play() -- เริ่ม :Play()
    require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.timeToNextAttack = 1
    game:GetService'VirtualUser':CaptureController()
    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
    require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.timeToNextAttack = 1
    game:GetService'VirtualUser':CaptureController()
    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
    require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.timeToNextAttack = 1
    game:GetService'VirtualUser':CaptureController()
    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
    require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework.CameraShaker).CameraShakeInstance.CameraShakeState = {FadingIn = 3,FadingOut =  2,Sustained = 0,Inactive = 1}
        game.Players[Select].Character.HumanoidRootPart.Size = Vector3.new(100,100,100)
        game.Players[Select].Character.HumanoidRootPart.Transparency = 1
    end
end
