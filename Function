       if LockLevel then
          if game.Players.LocalPlayer.Data.Level.Value >= LockLevelValue then
             game.Players.localPlayer:Kick("\n Auto Fram Completed Level : "..game.Players.LocalPlayer.Data.Level.Value.."\n Old Level : "..OldLevel)
          end
       end
    end
 end)
